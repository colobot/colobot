msgstr "\"new\" 参数错误"

msgid "\" [ \" expected"
msgstr "需要 \" [ \""

msgid "String missing"
msgstr "缺少字符串"

msgid "Incorrect index type"
msgstr "索引类型错误"

msgid "Private element"
msgstr "私有元素"

msgid "Public required"
msgstr "需要公开"

msgid "Expression expected after ="
msgstr "= 后需要表达式"

msgid "Ambiguous call to overloaded function"
msgstr "重载函数调用不明确"

msgid "Function needs return type \"void\""
msgstr "函数需要 \"void\" 返回类型"

msgid "Class name expected"
msgstr "需要类名称"

msgid "Non-void function needs \"return;\""
msgstr "非 void 函数需要 \"return;\""

msgid "This parameter needs a default value"
msgstr "此参数需要默认值"

msgid "Missing end quote"
msgstr "末尾缺少引号"

msgid "Unknown escape sequence"
msgstr "未知 Esc 序列"

msgid "Octal value out of range"
msgstr "八进制数值越界"

msgid "Missing hex digits after escape sequence"
msgstr "Esc 序列后缺少十六进制数字"

msgid "Hex value out of range"
msgstr "十六进制数值越界"

msgid "Invalid universal character name"
msgstr "通用字符名无效"

msgid "Empty character constant"
msgstr "字符常量为空"

msgid "Duplicate label in switch"
msgstr "switch 标签重复"

msgid "Dividing by zero"
msgstr "不可除以零"

msgid "Variable not initialized"
msgstr "变量未初始化"

msgid "Negative value rejected by \"throw\""
msgstr "\"throw\" 拒绝了负值"

msgid "The function returned no value"
msgstr "此函数无返回值"

msgid "No function running"
msgstr "没有运行中的函数"

msgid "Calling an unknown function"
msgstr "调用未知函数"

msgid "This class does not exist"
msgstr "类不存在"

msgid "Unknown Object"
msgstr "未知对象"

msgid "Operation impossible with value \"nan\""
msgstr "无法不带值 \"nan\" 执行操作"

msgid "Access beyond array limit"
msgstr "数组访问超限"

msgid "Stack overflow"
msgstr "栈上溢"

msgid "Illegal object"
msgstr "非法对象"

msgid "Can't open file"
msgstr "无法打开文件"

msgid "File not open"
msgstr "文件未打开"

msgid "Read error"
msgstr "读错误"

msgid "Write error"
msgstr "写错误"

msgid "Button %1"
msgstr "按钮 %1"

#, c-format
msgid "You have to use \"%1$s\" at least once in this exercise (used: %2$d)"
msgid_plural ""
"You have to use \"%1$s\" at least %3$d times in this exercise (used: %2$d)"
msgstr[0] "在此练习中至少使用 \"%1$s\" %3$d 次（已使用 %2$d 次）"

#, c-format
msgid "You cannot use \"%s\" in this exercise (used: %d)"
msgstr "不能在此练习中使用 \"%s\"（已使用 %d 次）"

#, c-format
msgid "You have to use \"%1$s\" at most once in this exercise (used: %2$d)"
msgid_plural ""
"You have to use \"%1$s\" at most %3$d times in this exercise (used: %2$d)"
msgstr[0] "在此练习中至多使用 \"%1$s\" %3$d 次（已使用 %2$d 次）"
